{
    "name": "apelang",
    "displayName": "apelang",
    "description": "ape language support",
    "version": "0.2.0",
    "publisher": "KrzysztofGabis",
    "icon": "images/logo.png",
    "engines": {
        "vscode": "^1.41.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "ape",
                "aliases": [
                    "ape",
                    "ape"
                ],
                "extensions": [
                    ".ape"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "ape",
                "scopeName": "source.ape",
                "path": "./syntaxes/ape.tmLanguage.json"
            }
        ]
    }
}
